  led48Timer = millis();                                  // Start des Timers
  led49Timer = millis();                                  // Start des Timers
  led50Timer = millis();                                  // Start des Timers
  led51Timer = millis();                                  // Start des Timers
  led52Timer = millis();                                  // Start des Timers
  led53Timer = millis();                                  // Start des Timers
}




  
void loop() {





  // LED 2 Handling             Alte Weser      F.
  digitalWrite(led2Pin, HIGH);
  




  // LED 3 Handling
  




  // LED 4 Handling             Wangerooge      F.
  digitalWrite(led4Pin, HIGH);




  // LED 5 Handling
  




  // LED 6 Handling




  // LED 7 Handling





  // LED 8 Handling
  





  // LED 9 Handling             Büsum      Iso.6s
  if (millis()>led9Timer+led9Timeout[led9Phase]){     // prüft den Timer
      led9Phase++;                                    // erhöht die Leuchtphase um 1
      if (led9Phase>led9Phases-1){                    // falls Leuchtphasenmaximum überschritten
        led9Phase = 0;                                // Leuchtphase zurücksetzen
      }
      led9Timer = millis();                           // Timer resetten
  }

  if (led9Phase==0) {                                                           // wenn led9State == 0, dann einfaden
    analogWrite(led9Pin, map(millis()-led9Timer,0,led9Timeout[0],0,60));
  } else if (led9Phase==1) {                                                    // wenn led9State == 1, dann an
    analogWrite(led9Pin, 60);
  } else if (led9Phase==2){                                                     // wenn led9State == 2, dann ausfaden
    analogWrite(led9Pin, 60-map(millis()-led9Timer,0,led9Timeout[0],0,60));  
  } else if (led9Phase==3) {                                                    // wenn led9State == 3, dann aus
    analogWrite(led9Pin, 0);
  }




  // LED 10 Handling
  if (millis()>led10Timer+led10Timeout[led10Phase]){     // prüft den Timer
      led10Phase++;                                      // erhöht die Leuchtphase um 1
      if (led10Phase>led10Phases-1){                     // falls Leuchtphasenmaximum überschritten
        led10Phase = 0;                                  // Leuchtphase zurücksetzen
      }
      led10Timer = millis();                             // Timer resetten
  }

  if (led10Phase==0) {                                                           // wenn led10State == 0, dann einfaden
    analogWrite(led10Pin, map(millis()-led10Timer,0,led10Timeout[0],0,60));
  } else if (led10Phase==1) {                                                    // wenn led10State == 1, dann an
    analogWrite(led10Pin, 60);
  } else if (led10Phase==2){                                                     // wenn led10State == 2, dann ausfaden
    analogWrite(led10Pin, 60-map(millis()-led10Timer,0,led10Timeout[0],0,60));  
  } else if (led10Phase==3) {                                                    // wenn led10State == 3, dann aus
    analogWrite(led10Pin, 0);
  } else if (led10Phase==4) {                                                    // wenn led10State == 4, dann einfaden
    analogWrite(led10Pin, map(millis()-led10Timer,0,led10Timeout[0],0,60));
  } else if (led10Phase==5) {                                                    // wenn led10State == 5, dann an
    analogWrite(led10Pin, 60);
  } else if (led10Phase==6){                                                     // wenn led10State == 6, dann ausfaden
    analogWrite(led10Pin, 60-map(millis()-led10Timer,0,led10Timeout[0],0,60));  
  } else if (led10Phase==7) {                                                    // wenn led10State == 7, dann aus
    analogWrite(led10Pin, 0);
  } else if (led10Phase==8) {                                                    // wenn led10State == 8, dann einfaden
    analogWrite(led10Pin, map(millis()-led10Timer,0,led10Timeout[0],0,60));
  } else if (led10Phase==9) {                                                    // wenn led10State == 9, dann an
    analogWrite(led10Pin, 60);
  } else if (led10Phase==10){                                                     // wenn led10State == 10, dann ausfaden
    analogWrite(led10Pin, 60-map(millis()-led10Timer,0,led10Timeout[0],0,60));  
  } else if (led10Phase==11) {                                                    // wenn led10State == 11, dann aus
    analogWrite(led10Pin, 0);
  } else if (led10Phase==12) {                                                    // wenn led10State == 13, dann einfaden
    analogWrite(led10Pin, map(millis()-led10Timer,0,led10Timeout[0],0,60));
  } else if (led10Phase==13) {                                                    // wenn led10State == 14, dann an
    analogWrite(led10Pin, 60);
  }





  // LED 11 Handling

  



  // LED 12 Handling





  // LED 13 Handling
  




  // LED 14 Handling





  // LED 15 Handling
  




  // LED 16 Handling





  // LED 17 Handling





  // LED 18 Handling





  // LED 19 Handling





  // LED 20 Handling
  




  // LED 21 Handling

  



  // LED 22 Handling





  // LED 23 Handling
  




  // LED 24 Handling





  // LED 25 Handling
  




  // LED 26 Handling





  // LED 27 Handling





  // LED 28 Handling





  // LED 29 Handling
  




  // LED 30 Handling
  




  // LED 31 Handling

  



  // LED 32 Handling





  // LED 33 Handling
  




  // LED 34 Handling





  // LED 35 Handling
  




  // LED 36 Handling





  // LED 37 Handling





  // LED 38 Handling





  // LED 39 Handling
  




  // LED 40 Handling
  




  // LED 41 Handling

  



  // LED 42 Handling





  // LED 43 Handling
  




  // LED 44 Handling





  // LED 45 Handling
  




  // LED 46 Handling





  // LED 47 Handling





  // LED 48 Handling





  // LED 49 Handling

  



  // LED 50 Handling





  // LED 51 Handling





  // LED 52 Handling





  // LED 53 Handling



  






  delay(20);
}
